{
  "repositories": [
    "trendyol/stove4k"
  ],
  "extends": [
    "config:base",
    "group:allNonMajor",
    ":dependencyDashboard"
  ],
  "branchPrefix": "renovate/",
  "platform": "github",
  "onboarding": false,
  "packageRules": [
   {
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch",
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ]
}
